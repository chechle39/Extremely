<XtraReportsLayoutSerializer SerializerVersion="19.2.4.0" Ref="1" ControlType="DevExpress.XtraReports.UI.XtraReport, DevExpress.XtraReports.v19.2, Version=19.2.4.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" Name="AccountBalanceReport" ScriptsSource="private void pageInfo1_PrintOnPage(object sender, DevExpress.XtraReports.UI.PrintOnPageEventArgs e) {&#xD;&#xA;  {&#xD;&#xA;            if (e.PageIndex == 0)&#xD;&#xA;                e.Cancel =true;&#xD;&#xA;        }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;private void label29_BeforePrint(object sender, System.Drawing.Printing.PrintEventArgs e) {&#xD;&#xA;&#x9;XRLabel label = (XRLabel)sender;&#xD;&#xA; &#xD;&#xA;     double FieldValue = Convert.ToDouble(GetCurrentColumnValue(&quot;Amount&quot;));  &#xD;&#xA;  &#x9;string txt = changeToWords(FieldValue.ToString());&#xD;&#xA;       (sender as XRLabel).Text = txt + &quot;Đồng&quot;;  &#xD;&#xA;&#xD;&#xA;}&#xD;&#xA;public static String changeToWords(String numb)  &#xD;&#xA;        {  &#xD;&#xA;            String val = &quot;&quot;;  &#xD;&#xA;            String wholeNo = numb;  &#xD;&#xA;            String points = &quot;&quot;;  &#xD;&#xA;            String andStr = &quot;&quot;;  &#xD;&#xA;            String pointStr = &quot;&quot;;  &#xD;&#xA;            String endStr = &quot;&quot;;  &#xD;&#xA;            try  &#xD;&#xA;            {  &#xD;&#xA;                int decimalPlace = numb.IndexOf(&quot;.&quot;);  &#xD;&#xA;                if (decimalPlace &gt; 0)  &#xD;&#xA;                {  &#xD;&#xA;                    wholeNo = numb.Substring(0, decimalPlace);  &#xD;&#xA;                    points = numb.Substring(decimalPlace + 1);  &#xD;&#xA;                    if (Convert.ToInt32(points) &gt; 0)  &#xD;&#xA;                    {  &#xD;&#xA;                                              &#xD;&#xA;                    }  &#xD;&#xA;                }  &#xD;&#xA;                val = String.Format(&quot;{0} {1}{2} {3}&quot;, translateWholeNumber(wholeNo).Trim(), andStr, pointStr, endStr);  &#xD;&#xA;  &#xD;&#xA;            }  &#xD;&#xA;            catch  &#xD;&#xA;            {  &#xD;&#xA;  &#xD;&#xA;            }  &#xD;&#xA;            return val;  &#xD;&#xA;        }  &#xD;&#xA;private static String translateWholeNumber(String number)  &#xD;&#xA;        {  &#xD;&#xA;            string word = &quot;&quot;;  &#xD;&#xA;            try  &#xD;&#xA;            {  &#xD;&#xA;                bool beginsZero = false;  &#xD;&#xA;                //tests for 0XX  &#xD;&#xA;                bool isDone = false;  &#xD;&#xA;                //test if already translated  &#xD;&#xA;                double dblAmt = (Convert.ToDouble(number));  &#xD;&#xA;                //if ((dblAmt &gt; 0) &amp;&amp; number.StartsWith(&quot;0&quot;))  &#xD;&#xA;                if (dblAmt &gt; 0)  &#xD;&#xA;                {  &#xD;&#xA;                    //test for zero or digit zero in a nuemric  &#xD;&#xA;                    beginsZero = number.StartsWith(&quot;0&quot;);  &#xD;&#xA;  &#xD;&#xA;                    int numDigits = number.Length;  &#xD;&#xA;                    int pos = 0;  &#xD;&#xA;                    //store digit grouping  &#xD;&#xA;                    String place = &quot;&quot;;  &#xD;&#xA;                    //digit grouping name:hundres,thousand,etc...  &#xD;&#xA;                    switch (numDigits)  &#xD;&#xA;                    {  &#xD;&#xA;                        case 1:  &#xD;&#xA;                            //ones' range  &#xD;&#xA;                            word = ones(number);  &#xD;&#xA;                            isDone = true;  &#xD;&#xA;                            break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                        case 2:  &#xD;&#xA;                            //tens' range  &#xD;&#xA;                            word = tens(number);  &#xD;&#xA;                            isDone = true;  &#xD;&#xA;                            break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                        case 3:  &#xD;&#xA;                            //hundreds' range  &#xD;&#xA;                            pos = (numDigits % 3) + 1;  &#xD;&#xA;                            place = &quot; Trăm &quot;;  &#xD;&#xA;                            break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                        //thousands' range  &#xD;&#xA;                        case 4:  &#xD;&#xA;                        case 5:  &#xD;&#xA;                        case 6:  &#xD;&#xA;                            pos = (numDigits % 4) + 1;  &#xD;&#xA;                            place = &quot; Ngàn &quot;;  &#xD;&#xA;                            break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                        //millions' range  &#xD;&#xA;                        case 7:  &#xD;&#xA;                        case 8:  &#xD;&#xA;                        case 9:  &#xD;&#xA;                            pos = (numDigits % 7) + 1;  &#xD;&#xA;                            place = &quot; Triệu &quot;;  &#xD;&#xA;                            break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                        case 10:  &#xD;&#xA;                            //Billions's range  &#xD;&#xA;                            pos = (numDigits % 10) + 1;  &#xD;&#xA;                            place = &quot; Tỷ &quot;;  &#xD;&#xA;                            break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;  &#xD;&#xA;                        default:  &#xD;&#xA;                            //add extra case options for anything above Billion...  &#xD;&#xA;                            isDone = true;  &#xD;&#xA;                            break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;  &#xD;&#xA;                    }  &#xD;&#xA;                    if (!isDone)  &#xD;&#xA;                    {  &#xD;&#xA;                        //if transalation is not done, continue...(Recursion comes in now!!)  &#xD;&#xA;                        word = translateWholeNumber(number.Substring(0, pos)) + place + translateWholeNumber(number.Substring(pos));  &#xD;&#xA;                        //check for trailing zeros  &#xD;&#xA;                        if (beginsZero)  &#xD;&#xA;                        {  &#xD;&#xA;                            word = &quot; and &quot; + word.Trim();  &#xD;&#xA;                        }  &#xD;&#xA;                    }  &#xD;&#xA;                    //ignore digit grouping names  &#xD;&#xA;                    if (word.Trim().Equals(place.Trim()))  &#xD;&#xA;                    {  &#xD;&#xA;                        word = &quot;&quot;;  &#xD;&#xA;                    }  &#xD;&#xA;                }  &#xD;&#xA;  &#xD;&#xA;            }  &#xD;&#xA;            catch  &#xD;&#xA;            {  &#xD;&#xA;  &#xD;&#xA;            }  &#xD;&#xA;            return word.Trim();  &#xD;&#xA;        } &#xD;&#xA; private static String tens(String digit)  &#xD;&#xA;        {  &#xD;&#xA;            int digt = Convert.ToInt32(digit);  &#xD;&#xA;            String name = null;  &#xD;&#xA;            switch (digt)  &#xD;&#xA;            {  &#xD;&#xA;                case 10:  &#xD;&#xA;                    name = &quot;Mười&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select        \  &#xD;&#xA;                case 11:  &#xD;&#xA;                    name = &quot;Mười Một&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                case 12:  &#xD;&#xA;                    name = &quot;Mười Hai&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                case 13:  &#xD;&#xA;                    name = &quot;Mười Ba&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                case 14:  &#xD;&#xA;                    name = &quot;Mười Bốn&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                case 15:  &#xD;&#xA;                    name = &quot;Mười Năm&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                case 16:  &#xD;&#xA;                    name = &quot;Mười Sáu&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                case 17:  &#xD;&#xA;                    name = &quot;Mười Bảy&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                case 18:  &#xD;&#xA;                    name = &quot;Mười Tám&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                case 19:  &#xD;&#xA;                    name = &quot;Mười Chín&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                case 20:  &#xD;&#xA;                    name = &quot;Hai Mươi&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                case 30:  &#xD;&#xA;                    name = &quot;Ba Mươi&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                case 40:  &#xD;&#xA;                    name = &quot;Bốn Mươi&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                case 50:  &#xD;&#xA;                    name = &quot;Năm Mươi&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                case 60:  &#xD;&#xA;                    name = &quot;Sáu Mươi&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                case 70:  &#xD;&#xA;                    name = &quot;Bảy Mươi&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                case 80:  &#xD;&#xA;                    name = &quot;Tám Mươi&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                case 90:  &#xD;&#xA;                    name = &quot;Chín Mươi&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                default:  &#xD;&#xA;                    if (digt &gt; 0)  &#xD;&#xA;                    {  &#xD;&#xA;                        name = (tens(digit.Substring(0, 1) + &quot;0&quot;) + &quot; &quot;) + ones(digit.Substring(1));  &#xD;&#xA;                    }  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;            }  &#xD;&#xA;            return name;  &#xD;&#xA;        }  &#xD;&#xA;        private static String ones(String digit)  &#xD;&#xA;        {  &#xD;&#xA;            int digt = Convert.ToInt32(digit);  &#xD;&#xA;            String name = &quot;&quot;;  &#xD;&#xA;            switch (digt)  &#xD;&#xA;            {  &#xD;&#xA;                case 1:  &#xD;&#xA;                    name = &quot;Một&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                case 2:  &#xD;&#xA;                    name = &quot;Hai&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                case 3:  &#xD;&#xA;                    name = &quot;Ba&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                case 4:  &#xD;&#xA;                    name = &quot;Bốn&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                case 5:  &#xD;&#xA;                    name = &quot;Năm&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                case 6:  &#xD;&#xA;                    name = &quot;Sáu&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                case 7:  &#xD;&#xA;                    name = &quot;Bảy&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                case 8:  &#xD;&#xA;                    name = &quot;Tám&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                case 9:  &#xD;&#xA;                    name = &quot;Chín&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;  &#xD;&#xA;            }  &#xD;&#xA;            return name;  &#xD;&#xA;        }  &#xD;&#xA;" Landscape="true" Margins="54, 60, 532, 64" PaperKind="A4" PageWidth="1169" PageHeight="827" Version="19.2" EventsInfo="|label29,BeforePrint,label29_BeforePrint" DataSource="#Ref-0" TextAlignment="TopCenter" Font="Arial, 9.75pt">
