     {  &#xD;&#xA;                        case 1:  &#xD;&#xA;                            //ones' range  &#xD;&#xA;                            word = ones(number);  &#xD;&#xA;                            isDone = true;  &#xD;&#xA;                            break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                        case 2:  &#xD;&#xA;                            //tens' range  &#xD;&#xA;                            word = tens(number);  &#xD;&#xA;                            isDone = true;  &#xD;&#xA;                            break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                        case 3:  &#xD;&#xA;                            //hundreds' range  &#xD;&#xA;                            pos = (numDigits % 3) + 1;  &#xD;&#xA;                            place = &quot; Trăm &quot;;  &#xD;&#xA;                            break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                        //thousands' range  &#xD;&#xA;                        case 4:  &#xD;&#xA;                        case 5:  &#xD;&#xA;                        case 6:  &#xD;&#xA;                            pos = (numDigits % 4) + 1;  &#xD;&#xA;                            place = &quot; Ngàn &quot;;  &#xD;&#xA;                            break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                        //millions' range  &#xD;&#xA;                        case 7:  &#xD;&#xA;                        case 8:  &#xD;&#xA;                        case 9:  &#xD;&#xA;                            pos = (numDigits % 7) + 1;  &#xD;&#xA;                            place = &quot; Triệu &quot;;  &#xD;&#xA;                            break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;                        case 10:  &#xD;&#xA;                            //Billions's range  &#xD;&#xA;                            pos = (numDigits % 10) + 1;  &#xD;&#xA;                            place = &quot; Tỷ &quot;;  &#xD;&#xA;                            break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;  &#xD;&#xA;                        default:  &#xD;&#xA;                            //add extra case options for anything above Billion...  &#xD;&#xA;                            isDone = true;  &#xD;&#xA;                            break; // TODO: might not be correct. Was : Exit Select  &#xD;&#xA;  &#xD;&#xA;                    }  &#xD;&#xA;                    if (!isDone)  &#xD;&#xA;                    {  &#xD;&#xA;                        //if transalation is not done, continue...(Recursion comes in now!!)  &#xD;&#xA;                        word = translateWholeNumber(number.Substring(0, pos)) + place + translateWholeNumber(number.Substring(pos));  &#xD;&#xA;                        //check for trailing zeros  &#xD;&#xA;                        if (beginsZero)  &#xD;&#xA;                        {  &#xD;&#xA;                            word = &quot; and &quot; + word.Trim();  &#xD;&#xA;                        }  &#xD;&#xA;                    }  &#xD;&#xA;                    //ignore digit grouping names  &#xD;&#xA;                    if (word.Trim().Equals(place.Trim()))  &#xD;&#xA;                    {  &#xD;&#xA;                        word = &quot;&quot;;  &#xD;&#xA;                    }  &#xD;&#xA;                }  &#xD;&#xA;  &#xD;&#xA;            }  &#xD;&#xA;            catch  &#xD;&#xA;            {  &#xD;&#xA;  &#xD;&#xA;            }  &#xD;&#xA;            return word.Trim();  &#xD;&#xA;        } &#xD;&#xA; private static String tens(String digit)  &#xD;&#xA;        {  &#xD;&#xA;            int digt = Convert.ToInt32(digit);  &#xD;&#xA;            String name = null;  &#xD;&#xA;            switch (digt)  &#xD;&#xA;            {  &#xD;&#xA;                case 10:  &#xD;&#xA;                    name = &quot;Mười&quot;;  &#xD;&#xA;                    break; // TODO: might not be correct. Was : Exit Select        \  &#xD;&#xA;                case 11:  &#